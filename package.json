{
  "name": "avaluos",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer",
    "build:release": "ng build --prod --build-optimizer --baseHref=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "tslint -p tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:scss": "stylelint --syntax scss 'src/**/*.scss' --fix",
    "hmr": "ng serve --hmr -c hmr --disable-host-check",
    "deploy": "angular-cli-ghpages --no-silent --dir=dist/ng-matero",
    "release": "npm run build:release && npm run deploy:ghpages",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular/animations": "^10.1.5",
    "@angular/cdk": "^10.1.1",
    "@angular/common": "^10.0.6",
    "@angular/compiler": "^10.0.6",
    "@angular/core": "^10.0.6",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "^10.0.6",
    "@angular/google-maps": "^9.2.4",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "^10.0.6",
    "@angular/platform-browser-dynamic": "^10.0.6",
    "@angular/router": "^10.0.6",
    "@iconify/icons-ic": "^1.0.14",
    "@ng-matero/extensions": "^0.9.3",
    "@ng-select/ng-select": "^3.7.3",
    "@ngx-formly/core": "^5.9.3",
    "@ngx-formly/material": "^5.9.3",
    "@ngx-progressbar/core": "~5.3.2",
    "@ngx-progressbar/router": "~5.3.2",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@visurel/iconify-angular": "0.0.7",
    "apexcharts": "^3.19.3",
    "core-js": "^3.6.5",
    "date-fns": "^2.15.0",
    "mat-table-filter": "^9.0.1",
    "ngx-mask": "^9.1.2",
    "ngx-popper": "^6.2.0",
    "ngx-take-until-destroy": "^5.4.0",
    "ngx-toastr": "^11.3.3",
    "photoviewer": "^3.4.0",
    "popper.js": "^1.16.1",
    "rxjs": "~6.6.0",
    "screenfull": "^4.2.1",
    "tailwindcss": "^1.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.4",
    "@angular/cli": "^10.0.4",
    "@angular/compiler-cli": "^10.0.6",
    "@angular/language-service": "^10.0.6",
    "@angularclass/hmr": "^2.1.3",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^8.3.4",
    "@ngx-formly/schematics": "^5.9.3",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.53",
    "@types/parse5": "^5.0.3",
    "angular-cli-ghpages": "^0.6.2",
